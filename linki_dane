    this.setCity = function (city) {
        localStorage.setItem('my City', city);


        localStorage.setItem('mojObiekt', JSON.stringify(obj))

    }
    this.getCity =function (city){
        localStorage.getItem('my City', city);

        var asString = localStorage.getItem('mojObiekt');
